{
	"name": "@lesjoursfr/exifr-cli",
	"version": "2.0.3",
	"description": "CLI for the exifr npm module.",
	"keywords": [
		"cli",
		"exifr",
		"exif",
		"reader"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/exifr-cli",
	"homepage": "https://github.com/lesjoursfr/exifr-cli#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/exifr-cli/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"exifr": "NODE_ENV=development ts-node src/index.ts",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc"
	},
	"bin": {
		"exifr": "bin/index.js"
	},
	"dependencies": {
		"cli-table": "^0.3.11",
		"exifr": "^7.1.3",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.2",
		"@types/cli-table": "^0.3.4",
		"@types/node": "^20.10.6",
		"@types/yargs": "^17.0.32",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.1.0"
}
