{
	"name": "@lesjoursfr/exifr-cli",
	"version": "1.0.0",
	"description": "CLI for the exifr npm module.",
	"keywords": [
		"cli",
		"exifr",
		"exif",
		"reader"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/exifr-cli",
	"homepage": "https://github.com/lesjoursfr/exifr-cli#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/exifr-cli/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=v16.15.0"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"exifr": "NODE_ENV=development ts-node src/index.ts",
		"lint": "npx eslint --ext .ts --fix src/",
		"build": "npx tsc"
	},
	"bin": {
		"exifr": "bin/index.js"
	},
	"dependencies": {
		"cli-table": "^0.3.11",
		"exifr": "^7.1.3",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.2",
		"@types/cli-table": "^0.3.0",
		"@types/node": "^16.11.38",
		"@types/yargs": "^17.0.10",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"ts-node": "^10.8.0",
		"typescript": "^4.7.2"
	},
	"packageManager": "yarn@3.2.1"
}
