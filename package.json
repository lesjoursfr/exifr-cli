{
	"name": "@lesjoursfr/exifr-cli",
	"version": "3.0.0",
	"description": "CLI for the exifr npm module.",
	"keywords": [
		"cli",
		"exifr",
		"exif",
		"reader"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/exifr-cli",
	"homepage": "https://github.com/lesjoursfr/exifr-cli#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/exifr-cli/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": "22.x || 24.x"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"exifr": "NODE_ENV=development ts-node src/index.ts",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc"
	},
	"bin": {
		"exifr": "bin/index.js"
	},
	"dependencies": {
		"cli-table": "^0.3.11",
		"exifr": "^7.1.3",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@tsconfig/node22": "^22.0.2",
		"@types/cli-table": "^0.3.4",
		"@types/node": "^24.9.2",
		"@types/yargs": "^17.0.34",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2"
	},
	"packageManager": "yarn@4.10.3"
}
