{
	"name": "@lesjoursfr/exifr-cli",
	"version": "1.0.3",
	"description": "CLI for the exifr npm module.",
	"keywords": [
		"cli",
		"exifr",
		"exif",
		"reader"
	],
	"author": "",
	"license": "MIT",
	"repository": "lesjoursfr/exifr-cli",
	"homepage": "https://github.com/lesjoursfr/exifr-cli#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/exifr-cli/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=16.15.1 || >=18.4.0"
	},
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"exifr": "NODE_ENV=development ts-node src/index.ts",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write .",
		"build": "tsc"
	},
	"bin": {
		"exifr": "bin/index.js"
	},
	"dependencies": {
		"cli-table": "^0.3.11",
		"exifr": "^7.1.3",
		"yargs": "^17.6.2"
	},
	"devDependencies": {
		"@tsconfig/node16": "^1.0.3",
		"@types/cli-table": "^0.3.1",
		"@types/node": "^18.11.18",
		"@types/yargs": "^17.0.17",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"prettier": "^2.8.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	},
	"packageManager": "yarn@3.3.1"
}
